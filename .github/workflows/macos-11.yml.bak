name: macOS

on:
  push:
    branches: [ master ]
  pull_request:
    branches: [ master ]

jobs:
  test:

    strategy:
      fail-fast: false
      matrix:
        version: [11]

    runs-on: macos-${{ matrix.version }}

    steps:
      - run: brew list
      - run: brew uninstall go@1.17
